  "devDependencies": {
    "@sneg240/eslint-config-custom": "workspace:*",
    "@turbo/gen": "1.10.15",
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.8.0",
    "@commitlint/prompt-cli": "17.7.2",
    "husky": "8.0.3",
    "eslint": "8.51.0",
    "prettier": "3.0.3",
    "turbo": "1.10.15"
  },
  "repository": "https://github.com/sneg240/frontend",
  "packageManager": "pnpm@8.6.10"
}
